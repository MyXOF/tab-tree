{
    "name": "Tab Tree",
    "version": "1.0",
    "description": "I don't like js but I have to learn it",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+E"
            }
        }
    },
    "manifest_version": 2
}